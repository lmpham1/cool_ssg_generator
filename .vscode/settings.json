{
    "cSpell.words": [
        "pytest",
        "rglob"
    ],
    "python.formatting.provider": "black",
    "editor.formatOnSave": true,
    "python.linting.flake8Enabled": true,
    "python.linting.enabled": true,
    "python.linting.ignorePatterns": [
        "**/site-packages/**/*.py",
        ".vscode/*.py",
        ".git",
        ".github",
    ]
}